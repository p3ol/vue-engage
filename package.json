{
  "name": "@poool/vue-engage",
  "version": "1.0.0",
  "description": "Plugin to easily add Poool engage to your Vue app âœ¨",
  "repository": "https://github.com/p3ol/vue-engage.git",
  "author": "Nicolas Auger <nicolas@poool.fr",
  "license": "MIT",
  "main": "dist/poool-vue-engage.cjs.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/poool-vue-engage.cjs.js"
    }
  },
  "engines": {
    "node": "20 || 22 || 24"
  },
  "workspaces": [
    "examples/*"
  ],
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "@junipero/core": "3.12.3",
    "@types/poool-engage": "2.2.0"
  },
  "devDependencies": {
    "@poool/eslint-config": "5.0.7",
    "@rollup/plugin-commonjs": "29.0.0",
    "@rollup/plugin-node-resolve": "16.0.3",
    "@rollup/plugin-swc": "0.4.0",
    "@rollup/plugin-terser": "0.4.4",
    "@swc/core": "1.15.3",
    "@types/node": "25.0.1",
    "@vitejs/plugin-vue": "6.0.2",
    "@vue/eslint-config-typescript": "14.6.0",
    "@vue/tsconfig": "0.8.1",
    "commit-and-tag-version": "12.6.1",
    "eslint": "9.39.1",
    "eslint-plugin-vue": "10.6.2",
    "rollup": "4.53.3",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "vite": "7.2.7",
    "vue": "3.5.25",
    "vue-eslint-parser": "10.2.0",
    "vue-tsc": "3.1.8"
  },
  "scripts": {
    "type-check": "vue-tsc --build --force",
    "clean": "rm -rf ./dist || true",
    "build": "yarn clean && yarn build:code && yarn build:types",
    "build:code": "rollup --configPlugin @rollup/plugin-swc -c",
    "build:types": "tsc --project ./tsconfig.build.json",
    "prepack": "yarn build && yarn lint && yarn tsc",
    "lint": "eslint --max-warnings=0 .",
    "release": "commit-and-tag-version && git push --follow-tags",
    "example:basic": "yarn workspace poool-vue-engage-basic-example serve",
    "example:nuxt": "yarn workspace poool-vue-engage-nuxt-example serve"
  },
  "packageManager": "yarn@4.12.0"
}
